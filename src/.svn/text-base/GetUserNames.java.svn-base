import java.awt.List;
import java.io.BufferedWriter;
import java.io.FileWriter;
import java.util.ArrayList;
import java.util.Vector;


public class GetUserNames {
	public String getNames(String names){
		Vector<String> namesVector = new Vector<String>();
		
		namesVector.add(names);
		StringBuffer buffer = new StringBuffer();
		String x;
		for (int i = 0; i <= namesVector.size() - 1; i++) {
			String allNames = namesVector.get(i);
			buffer.append(allNames);
			System.out.println(allNames);
			
		}
		x = buffer.toString() ;
		return x;
    }
}
